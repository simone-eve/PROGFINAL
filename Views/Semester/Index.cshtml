@model IEnumerable<PROG6212POE.Models.Semester>

@{
    ViewData["Title"] = "Index";
}

<h1>View Semester</h1>

<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.SemesterWeeks)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.SemesterDate)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.SemesterWeeks)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.SemesterDate)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.SemesterId">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.SemesterId">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.SemesterId">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
